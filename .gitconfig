	email = ashton.sidhu1994@gmail.com
	name = Ashton Sidhu
[alias]
	co = checkout
	cob = checkout -b
	br = branch
	ci = commit
	st = status
	gpsup = "git push -u origin $(git symbolic-ref --short HEAD)"
	undo = "! f() { git clean -df && git checkout -- .}; f"
	reset-to-remote = "! git clean -d -f && git fetch origin && git checkout master && git reset --hard origin/master"
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
[push]
	autoSetupRemote = true
